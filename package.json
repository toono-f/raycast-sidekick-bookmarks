{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-sidekick-bookmarks",
  "title": "raycast sidekick bookmarks",
  "description": "raycast sidekick bookmarks",
  "icon": "sidekick-logo.png",
  "author": "toono-f",
  "license": "MIT",
  "categories": [
    "Applications",
    "Developer Tools",
    "Productivity",
    "System"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Sidekick bookmarks search",
      "description": "Sidekick bookmarks search.",
      "mode": "view",
      "preferences": [
        {
          "name": "firstInResults",
          "type": "dropdown",
          "required": false,
          "title": "First in results",
          "description": "Which browser history shows up first in results",
          "data": [
            {
              "title": "Sidekick",
              "value": "Sidekick"
            }
          ]
        },
        {
          "name": "defaultBrowser",
          "type": "dropdown",
          "required": false,
          "title": "Default browser to open in",
          "description": "Which browser opens upon pressing enter on an entry",
          "data": [
            {
              "title": "Sidekick",
              "value": "Sidekick"
            }
          ]
        },
        {
          "name": "enableSidekick",
          "type": "checkbox",
          "required": false,
          "description": "Enable Firefox history to show up in results",
          "default": false,
          "label": "Sidekick"
        },
        {
          "name": "profilePathSidekick",
          "type": "textfield",
          "required": false,
          "title": "Sidekick Profile Path",
          "description": "Path to Sidekick profile folder. Leave empty to use default profile. Default at /Users/username/Library/Application Support/Sidekick/Default",
          "placeholder": "/Users/username/Library/Application Support/Sidekick/Default"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "1.60.1",
    "@raycast/utils": "1.10.0"
  },
  "devDependencies": {
    "@types/node": "20.8.7",
    "@types/react": "18.2.29",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "scripts": {
    "dev": "ray develop",
    "build": "ray build -e dist",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "publish": "ray publish"
  }
}
